{
  "provider": {
    "name": "registration_api_provider"
  },
  "consumer": {
    "name": "android_app"
  },
  "interactions": [
    {
      "description": "a registration request",
      "request": {
        "method": "POST",
        "path": "/api/devices/registrations",
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": {
          "pushToken": "a-valid-token"
        },
        "matchingRules": {
          "body": {
            "$.pushToken": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": ".*"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "response": {
        "status": 204
      },
      "providerStates": [
        {
          "name": "no existing registration"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.10"
    }
  }
}